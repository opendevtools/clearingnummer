{
  "name": "@opendevtools/clearingnummer",
  "version": "1.1.0",
  "description": "Clearing numbers for Swedish banks",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsc -w",
    "lint": "eslint 'lib/**/*.{ts,tsx}'",
    "test": "is-ci test:ci test:watch",
    "test:ci": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc --declaration",
    "prepare": "npm run build",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opendevtools/clearingnummer.git"
  },
  "keywords": [
    "Clearing",
    "Number",
    "Swedish",
    "Banks",
    "Bank",
    "Clearingnummer",
    "Svensk bank"
  ],
  "author": "Rickard Natt och Dag <nattochdag@hey.com> (http://willcodefor.beer)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opendevtools/clearingnummer/issues"
  },
  "homepage": "https://github.com/opendevtools/clearingnummer#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "26.0.15",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "all-contributors-cli": "6.19.0",
    "babel-eslint": "10.1.0",
    "babel-preset-env": "1.7.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "is-ci-cli": "2.1.2",
    "jest": "26.6.3",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "semantic-release": "17.2.3",
    "typescript": "4.0.5"
  }
}
