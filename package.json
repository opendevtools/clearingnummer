{
  "name": "clearingnummer",
  "version": "0.6.0",
  "description": "Clearing numbers for Swedish banks",
  "main": "./dist/index.js",
  "scripts": {
    "start": "tsc -w",
    "lint": "tslint ./lib/**/*.js ./test/**/*.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "tsc",
    "prepublish": "npm test && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/believer/clearingnummer.git"
  },
  "keywords": [
    "Clearing",
    "Number",
    "Swedish",
    "Banks",
    "Bank",
    "Clearingnummer",
    "Svensk bank"
  ],
  "author": "Rickard Laurin <rickard.laurin@gmail.com> (http://rickardlaurin.se/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/believer/clearingnummer/issues"
  },
  "homepage": "https://github.com/believer/clearingnummer#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "22.1.1",
    "babel-eslint": "7.2.3",
    "babel-preset-env": "1.6.0",
    "eslint": "1.10.3",
    "jest": "20.0.4",
    "ts-jest": "^22.0.3",
    "tslint": "5.9.1",
    "typescript": "2.7.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  }
}
