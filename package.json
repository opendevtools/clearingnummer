    "Svensk bank"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsc -w",
    "lint": "bunx @biomejs/biome check lib",
    "test": "bun test",
    "prebuild:js": "rimraf dist",
