{
  "name": "clearingnummer",
  "version": "0.6.2",
  "description": "Clearing numbers for Swedish banks",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "lint": "tslint ./lib/**/*.js ./test/**/*.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "tsc --declaration",
    "prepare": "npm test && npm run lint && npm run build",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/believer/clearingnummer.git"
  },
  "keywords": [
    "Clearing",
    "Number",
    "Swedish",
    "Banks",
    "Bank",
    "Clearingnummer",
    "Svensk bank"
  ],
  "author": "Rickard Laurin <rickard.laurin@gmail.com> (http://rickardlaurin.se/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/believer/clearingnummer/issues"
  },
  "homepage": "https://github.com/believer/clearingnummer#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "22.2.2",
    "all-contributors-cli": "4.11.1",
    "babel-eslint": "8.2.2",
    "babel-preset-env": "1.6.1",
    "eslint": "4.19.1",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.0.4",
    "prettier": "1.11.1",
    "ts-jest": "22.4.2",
    "tslint": "5.9.1",
    "typescript": "2.8.1",
    "validate-commit-msg": "2.14.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  }
}
