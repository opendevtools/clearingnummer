{
  "name": "clearingnummer",
  "version": "0.6.2",
  "description": "Clearing numbers for Swedish banks",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "lint": "eslint 'lib/**/*.{ts,tsx}'",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:ci": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc --declaration",
    "prepare": "npm run build",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/believer/clearingnummer.git"
  },
  "keywords": [
    "Clearing",
    "Number",
    "Swedish",
    "Banks",
    "Bank",
    "Clearingnummer",
    "Svensk bank"
  ],
  "author": "Rickard Laurin <rickard.laurin@gmail.com> (http://rickardlaurin.se/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/believer/clearingnummer/issues"
  },
  "homepage": "https://github.com/believer/clearingnummer#readme",
  "dependencies": {
    "merge": "1.2.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "25.1.2",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "all-contributors-cli": "6.13.0",
    "babel-eslint": "10.0.3",
    "babel-preset-env": "1.7.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "is-ci-cli": "2.0.0",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "prettier": "1.19.1",
    "semantic-release": "17.0.3",
    "ts-jest": "25.2.0",
    "typescript": "3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.ts"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  }
}
