{
  "name": "clearingnummer",
  "version": "0.6.2",
  "description": "Clearing numbers for Swedish banks",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "lint": "tslint ./lib/**/*.js ./test/**/*.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "tsc --declaration",
    "prepare": "npm test && npm run lint && npm run build",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/believer/clearingnummer.git"
  },
  "keywords": [
    "Clearing",
    "Number",
    "Swedish",
    "Banks",
    "Bank",
    "Clearingnummer",
    "Svensk bank"
  ],
  "author": "Rickard Laurin <rickard.laurin@gmail.com> (http://rickardlaurin.se/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/believer/clearingnummer/issues"
  },
  "homepage": "https://github.com/believer/clearingnummer#readme",
  "dependencies": {
    "merge": "1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@types/jest": "24.0.15",
    "all-contributors-cli": "6.8.1",
    "babel-eslint": "10.0.2",
    "babel-preset-env": "1.7.0",
    "eslint": "6.1.0",
    "husky": "3.0.1",
    "jest": "24.8.0",
    "lint-staged": "9.2.0",
    "prettier": "1.18.2",
    "semantic-release": "15.13.18",
    "travis-deploy-once": "5.0.11",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  }
}
