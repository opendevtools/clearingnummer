{
  "name": "@opendevtools/clearingnummer",
  "version": "1.3.0",
  "description": "Clearing numbers for Swedish banks",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsc -w",
    "lint": "eslint 'lib/**/*.{ts,tsx}'",
    "test": "is-ci test:ci test:watch",
    "test:ci": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc --declaration",
    "prepare": "npm run build",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opendevtools/clearingnummer.git"
  },
  "keywords": [
    "Clearing",
    "Number",
    "Swedish",
    "Banks",
    "Bank",
    "Clearingnummer",
    "Svensk bank"
  ],
  "author": "Rickard Natt och Dag <rickard@nattochdag.dev> (https://willcodefor.beer)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opendevtools/clearingnummer/issues"
  },
  "homepage": "https://github.com/opendevtools/clearingnummer#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "7.16.5",
    "@babel/preset-typescript": "7.14.5",
    "@types/jest": "26.0.24",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "all-contributors-cli": "6.20.0",
    "babel-eslint": "10.1.0",
    "babel-preset-env": "1.7.0",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "is-ci-cli": "2.2.0",
    "jest": "27.0.6",
    "lint-staged": "11.0.1",
    "pinst": "^2.1.6",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  }
}
